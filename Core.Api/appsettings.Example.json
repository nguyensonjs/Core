{
  "ConnectionStrings": {
    "DefaultConnection": "Server=<your-server-name>,<port>;Database=<your-database-name>;User Id=<your-username>;Password=<your-password>;TrustServerCertificate=<true-or-false>;"
  },
  "EmailConfiguration": {
    "From": "<your-email-address>",
    "FromDisplayName": "<your-display-name>",
    "SmtpServer": "<your-smtp-server>",
    "Port": "<smtp-port-number>",
    "Username": "<your-smtp-username>",
    "Password": "<your-smtp-password>",
    "EnableSsl": "<true-or-false>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "<log-level>",
      "Microsoft.AspNetCore": "<log-level>"
    }
  }
}